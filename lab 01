// bai 2

#include <iostream> ;

using namespace std;
int main () 
{
	cout << "**      * \n"  ;
	cout << "*  *    * \n"  ; 
	cout << "*    *  * \n"  ;
	cout << "*      ** \n\n"  ;
	
	cout << "     **  \n "      ; 
	cout << "   *  *   \n"    ; 
	cout << "   ******     \n " ; 
	cout << " *      *      \n\n" ; 
	
	cout << " **                ** \n" ; 
	cout << " *  *           *   *\n ";
	cout << "*    *      *      *\n";
	cout << " *      * *         * \n    "; 
	
	return 0 ; 
	 
	
}
